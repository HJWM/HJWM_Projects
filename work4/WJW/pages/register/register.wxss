/* pages/register/register.wxss */

page {
  background: #f0f0f0;
}

.row {
  margin-top: 20rpx;
  overflow: hidden;
  line-height: 100rpx;
  border-bottom: 1rpx solid #ccc;
  margin-left: 20rpx;
  margin-right: 20rpx;
  color: #777;
  background: #fff;
}

.title-css{
width: 100%;
height: 200rpx;
text-align: center;
line-height: 200rpx;
}

.info-input {
  height: 100rpx;
  margin-left: 50rpx;
  color: #777;
  float: left;
}

.info-input1 {
  height: 100rpx;
  margin-left: 50rpx;
  color: #777;
  float: left;
  width: 420rpx;
}

.button {
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  background: #3f9;
  margin-left: 10rpx;
  margin-top: 15rpx;
  color: #fff;
   margin:20px auto;
}

.submit {
  margin-top: 50rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  background: #0cf;
  color: #fff;
}

.success {
  background: #fff;
}

.cheer {
  text-align: center;
  line-height: 400rpx;
  font-size: 60rpx;
  position: relative;
}

.return {
  margin: 20rpx;
}
